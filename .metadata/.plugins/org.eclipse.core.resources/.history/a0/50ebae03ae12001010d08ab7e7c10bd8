package oops.constructor;

public class Default {
	int a;
	  boolean b;

	  public static void main(String[] args) {

	    // calls default constructor
	    Main obj = new Main();

	    System.out.println("Default Value:");
	    System.out.println("a = " + obj.a);
	    System.out.println("b = " + obj.b);
	  }
}
